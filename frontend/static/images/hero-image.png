    <circle cx="50" cy="270" r="15" fill="#FBBC05"/>
    <circle cx="550" cy="210" r="15" fill="#34A853"/>
    
    <!-- Input Area -->
    <rect x="70" y="320" width="400" height="40" rx="20" fill="#F5F7FA" stroke="#E4E9F2" stroke-width="2"/>
    <rect x="90" y="335" width="150" height="10" rx="5" fill="#8F9BB3" fill-opacity="0.5"/>
    <circle cx="520" cy="340" r="15" fill="#4285F4"/>
    <path d="M525 335L515 340L525 345V335Z" fill="white"/>
